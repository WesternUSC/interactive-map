{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "build"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*).js",
        "headers": { "content-type": "application/javascript" },
        "dest": "/$1.js"
      },
      {
        "src": "/(.*).css",
        "headers": { "content-type": "text/css" },
        "dest": "/$1.css"
      },
      {
        "src": "/(.*)",
        "headers": { "cache-control": "s-maxage=0" },
        "dest": "/index.html"
      }
    ]
  }
  